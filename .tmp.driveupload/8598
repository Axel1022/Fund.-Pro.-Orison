class Convertor
{
    public static void TemperaturasDesicion(int N)
    {
        if (N == 1)
        {
            TextoGrande.DeFa();
        }
        else if (N == 2)
        {
            TextoGrande.DeCe();
        }
        else if (N == 3)
        {
            TextoGrande.DeKe();
        }
    }

}


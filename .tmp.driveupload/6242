using System;

{
    int A, B;
    Console.Write("Ingrese el primer numero:");
    A = int.Parse(Console.ReadLine());
    Console.Write("Ingrese el segundo numero:");
    B = int.Parse(Console.ReadLine());

    if (A > B)
    {
        Console.Write(A + " es mayor que " + B);

    }
    else
    {
        Console.Write(B + " es mayor que " + A);
    }

}


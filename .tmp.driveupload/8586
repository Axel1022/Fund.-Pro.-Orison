    public static void creador()
    {
        Console.WriteLine(@"
        ╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋┏┓╋┏┳━━━┳━━━┳┓╋┏┳━━━┓╋╋╋╋┏━━━┳━━━┳━━━┓╋╋╋╋┏━━━┳━┓┏━┳━━━┳┓
        ╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋┃┃╋┃┃┏━━┫┏━┓┃┃╋┃┃┏━┓┃╋╋╋╋┃┏━┓┃┏━┓┃┏━┓┃╋╋╋╋┃┏━┓┣┓┗┛┏┫┏━━┫┃
        ╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋╋┃┗━┛┃┗━━┫┃╋┗┫┗━┛┃┃╋┃┃╋╋╋╋┃┗━┛┃┃╋┃┃┗━┛┃╋┏┓╋┃┃╋┃┃┗┓┏┛┃┗━━┫┃
        ┏━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┫┏━┓┃┏━━┫┃╋┏┫┏━┓┃┃╋┃┃┏━━┓┃┏━━┫┃╋┃┃┏┓┏┛┏┛┗┓┃┗━┛┃┏┛┗┓┃┏━━┫┃╋┏┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┳━━┓
        ┗━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┫┃╋┃┃┗━━┫┗━┛┃┃╋┃┃┗━┛┃┗━━┛┃┃╋╋┃┗━┛┃┃┃┗┓┗┓┏┛┃┏━┓┣┛┏┓┗┫┗━━┫┗━┛┣━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┻━━┛
